{
  "productName": "QR Printer App",
  "version": "0.1.0",
  "identifier": "com.example.qrprinterapp",
  "build": {
    "beforeDevCommand": "npm run web",
    "beforeBuildCommand": "npm run web-build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 1000,
        "resizable": true,
        "title": "QR Printer App",
        "width": 1600,
        "minHeight": 800,
        "minWidth": 1200,
        "center": true,
        "maximized": true,
        "decorations": true,
        "alwaysOnTop": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["deb"],
    "icon": []
  }
}
